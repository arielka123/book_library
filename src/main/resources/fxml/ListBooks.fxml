<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.moja.biblioteczka.controllers.ListBooksController">
   <children>
      <HBox prefHeight="59.0" prefWidth="662.0" spacing="10.0">
         <children>
            <Label text="%category.book" />
            <ComboBox prefWidth="150.0" />
            <Label text="%author.book" />
            <ComboBox prefWidth="150.0" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
      <TableView maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
        <columns>
          <TableColumn prefWidth="75.0" text="%title.book" />
          <TableColumn prefWidth="98.6666488647461" text="%desc.book" />
            <TableColumn minWidth="0.0" prefWidth="102.66670227050781" text="%author.book" />
            <TableColumn prefWidth="101.3333740234375" text="%category.book" />
            <TableColumn prefWidth="78.0" text="%rating.book" />
            <TableColumn prefWidth="89.33334350585938" text="%isbn.book" />
            <TableColumn prefWidth="104.0" text="%release.book" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="20.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
